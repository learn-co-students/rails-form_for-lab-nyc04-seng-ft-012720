<h1>School Class Form</h1>

<%= form_for @student do |f| %>
  <ul>
     <li>
     <%= f.label :first_name %>
     <%= f.text_field :first_name %>
     </li>
     <li>
     <%= f.label :last_name %>
     <%= f.text_field :last_name %>
     </li>
     <li>
     <%= f.submit %>
     </li>
  </ul>
<% end %>
